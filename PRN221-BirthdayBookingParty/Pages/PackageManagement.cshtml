@page
@model PRN221_BirthdayBookingParty.Pages.PackageManagementModel
@{
	<table class="table table-striped table-bordered">
						<thead>
							<tr>
								<th>PackageID</th>
								<th>PackageName</th>
								<th>PackageType</th>
								<th colspan="2">Action</th>
							</tr>
						</thead>

						<tbody>
			@foreach (var Package in Model.Packages)
			{
													<tr>
																<td>@Package.PackageId</td>
																<td>@Package.PackageName</td>
																<td>@Package.PackageType</td>
																						<td><a class="btn btn-primary" asp-page="/PackageUpdate/@Package.PackageId">Update</a></td>
																								<td><a class="btn btn-danger" asp-route-id="@Package.PackageId" asp-page="/PackageDelete">Delete</a></td>
													</tr>
			}
						</tbody>
					</table>
}
